void 	swap(double *n1, double *n2)
{
	double swap = *n1;
	*n1 = *n2;
	*n2 = swap;
}

void	sort_array(double *raw_sensor_value, size_t size)
{
	for (int i = 0; i < size - 1; ++i) {
		for (int j = i + 1; j < size; ++j) {
			if (arr[i] > arr[j])
				swap(&arr[i], &arr[j]);
		}
	}
}

